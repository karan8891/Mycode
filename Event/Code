from airflow.providers.google.cloud.operators.cloud_storage_transfer_service import (
    CloudDataTransferServiceUpdateJobOperator,
)

run_job = CloudDataTransferServiceUpdateJobOperator(
    task_id="run_sts_job",
    job_name="{{ task_instance.xcom_pull(task_ids='create_sts_job')['name'] }}",
    project_id="your-cdmp-project-id",
    update_body={"status": "ENABLED"},
    update_mask="status",
)




from airflow import DAG
from airflow.operators.python import PythonOperator
from airflow.utils.dates import days_ago

def print_google_provider_version():
    from airflow.providers.google import __version__ as g_version
    print("Google provider version:", g_version)

with DAG(
    dag_id="check_google_provider_version_dag",
    start_date=days_ago(1),
    schedule_interval=None,
    catchup=False,
) as dag:

    print_version = PythonOperator(
        task_id="print_g_provider_version",
        python_callable=print_google_provider_version,
    )
